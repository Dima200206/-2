      "cell_type": "code",
      "source": [
        "with torch.no_grad():\n",
        "    y_pred = model(X_test_tensor)\n",
        "    y_pred_class = (y_pred > 0.5).float()\n",
        "\n",
        "accuracy = accuracy_score(y_test_tensor, y_pred_class)\n",
        "loss_value = criterion(y_pred, y_test_tensor).item()\n",
        "\n",
